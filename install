path=/var/stack
error_check1=0
error_check2=0
error_check3=0
error_check4=0
error_check5=0
error_check6=0
error_check7=0
if !(grep -x "alias h='source h'" ~/.bashrc 2>/dev/null 1>/dev/null); then
  error_check1=0
else
  error_check1=1
fi
if !(grep -x "alias f='source f'" ~/.bashrc 2>/dev/null 1>/dev/null); then
  error_check2=0
else
  error_check2=1
fi
if !(grep -x "alias b='source b'" ~/.bashrc 2>/dev/null 1>/dev/null); then
  error_check3=0
else
  error_check3=1
fi
if !(grep -x "alias cd='source c \$@'" ~/.bashrc 2>/dev/null 1>/dev/null); then
  error_check4=0
else
  error_check4=1
fi
if !(grep -x "alias l='ll -is'" ~/.bashrc 2>/dev/null 1>/dev/null); then
  error_check5=0
else
  error_check5=1
fi
if !(ls $path/home 1>/dev/null 2>/dev/null); then
  error_check6=0
else
  error_check6=1
fi
if !(ls $path/front 1>/dev/null 2>/dev/null); then
  error_check7=0
else
  error_check7=1
fi

if [ $error_check1 = 0 ] && [ $error_check2 = 0 ] && [ $error_check3 = 0 ] && [ $error_check4 = 0 ] && [ $error_check5 = 0 ] && [ $error_check6 = 0 ] && [ $error_check7 = 0 ]; then
  mkdir -p $path
  echo "alias h='source h'" >> ~/.bashrc
  echo "alias f='source f'" >> ~/.bashrc
  echo "alias b='source b'" >> ~/.bashrc
  echo "alias cd='source c \$@'" >> ~/.bashrc
  echo "alias l='ll -is'" >> ~/.bashrc
  cp h /usr/bin
  cp f /usr/bin
  cp b /usr/bin
  cp c /usr/bin
  source ~/.bashrc
else
  if [ $error_check1 = 1 ]; then
    echo "Already existing 'h' command!"
  elif [ $error_check2 = 1 ]; then
    echo "Already existing 'f' command!"
  elif [ $error_check3 = 1 ]; then
    echo "Already existing 'b' command!"
  elif [ $error_check4 = 1 ]; then
    echo "Already existing 'cd' command!"
  elif [ $error_check5 = 1 ]; then
    echo "Already existing 'l' command!"
  elif [ $error_check6 = 1 ]; then
    echo "Already existing '$path/home' file!"
  elif [ $error_check7 = 1 ]; then
    echo "Already existing '$path/front' file!"
  fi
fi

